{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "border": "4",
    "accent": "3",
    "text": "7",
    "secondary": "5",
    "success": "2",
    "error": "1",
    "info": "6"
  },
  "blocks": [
    {
      "alignment": "left",
      "leading_diamond": "<p:border>\u250f</>",
      "segments": [
        {
          "foreground": "p:border",
          "style": "diamond",
          "template": "[<p:text>\ueb99</> {{ .UserName }} @ <p:text>\uf108</> {{ .HostName }}]",
          "type": "session"
        },
        {
          "foreground": "p:info",
          "style": "diamond",
          "template": "[<p:text>\ue266</> RAM: {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB]",
          "type": "sysinfo"
        },
        {
          "foreground": "p:accent",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}p:border{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:accent{{end}}",
            "{{if eq \"Full\" .State.String}}p:success{{end}}"
          ],
          "options": {
            "charged_icon": "<p:text>\ue22f</> ",
            "charging_icon": "\ueb2d ",
            "discharging_icon": "<p:accent>\uf244</> "
          },
          "style": "plain",
          "template": "[{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295]",
          "type": "battery"
        },
        {
          "foreground": "p:info",
          "options": {
            "style": "dallas",
            "threshold": 0
          },
          "style": "diamond",
          "template": "[<p:text>\uf252</> {{ .FormattedMs }}s]",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "leading_diamond": "<p:border>\u2523</>",
      "segments": [
        {
          "foreground": "p:border",
          "style": "diamond",
          "template": "[<p:text>\uebd8</> Subscription: <p:accent>{{ .Name }}</>]",
          "type": "az"
        },
        {
          "foreground": "p:border",
          "style": "diamond",
          "template": "[<p:text>\uebd8</> Azure Dev CLI: <p:accent>{{ .DefaultEnvironment }}</>]",
          "type": "azd"
        },
        {
          "foreground": "p:border",
          "style": "diamond",
          "template": "[<p:text>\uf270</> <p:accent>{{ .Profile }}</>{{if .Region}}@<p:accent>{{ .Region }}</>{{ end }}]",
          "type": "aws"
        },
        {
          "foreground": "p:border",
          "style": "diamond",
          "template": "[{{ if .Error }}{{ .Error }}{{ else }}<p:text>\uf1a0</> <p:accent>{{ .Project }}</>{{ end }}]",
          "type": "gcp"
        },
        {
          "foreground": "p:secondary",
          "style": "diamond",
          "template": "[<p:text>\uf10fe</> <p:accent>{{.Context}}</>]",
          "type": "kubectl"
        },
        {
          "foreground": "p:border",
          "style": "diamond",
          "template": "[<p:text>îˆµ </> <p:accent>{{ .Full }}</>] ",
          "type": "python"
        },
        {
          "foreground": "p:border",
          "style": "diamond",
          "template": "{{ if ne .Type \"unknown\" }}[<p:text>\ue843 </> <p:accent>nix-{{ .Type }}</>]{{ end }}",
          "type": "nix-shell"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "leading_diamond": "<p:border>\u2523</>",
      "segments": [
        {
          "foreground": "p:border",
          "options": {
            "style": "full"
          },
          "style": "diamond",
          "template": "[<p:text>\uf115</> <p:success>{{ .Path }}</>]",
          "type": "path"
        },
        {
          "foreground": "p:secondary",
          "options": {
            "fetch_status": true
          },
          "style": "plain",
          "template": "[<p:text>{{ .UpstreamIcon }} </>{{ .HEAD }}{{ if .Working.Changed }} <p:text>\uf044</> {{ .Working.String }}{{ end }}]",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "leading_diamond": "<p:border>\u2514\u2500</>",
      "segments": [
        {
          "style": "diamond",
          "template": "<p:accent>[#]</>",
          "type": "root"
        },
        {
          "foreground": "p:error",
          "style": "diamond",
          "template": "[<p:text>\uea6c</> Error]",
          "type": "status"
        },
        {
          "style": "diamond",
          "template": ">",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "error_line": {
    "template": "<p:error>\uea87</> "
  },
  "version": 3
}