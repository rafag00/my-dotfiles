{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "small",
        "padding": {
            "top": 1,
            "left": 2,
            "right": 2
        },
        "color": {
           "1": "cyan"
        }
    },
    "display": {
        "separator": " "
    },
    "modules": [
        {
            "key": "{#34}╭───────────╮",
            "type": "custom"
        },
        {
            "key": "{#34}│ {#31} user    {#keys}{#34}│",
            "type": "title",
            "format": "{user-name}"
        },
        {
            "key": "{#34}│ {#32}󰇅 hname   {#keys}{#34}│",
            "type": "title",
            "format": "{host-name}"
        },
        {
            "key": "{#34}│ {#33}󰅐 uptime  {#keys}{#34}│",
            "type": "uptime"
        },
        {
            "key": "{#34}│ {#34}{icon} distro  {#keys}{#34}│",
            "type": "os"
        },
        {
            "key": "{#34}│ {#35} kernel  {#keys}{#34}│",
            "type": "kernel"
        },
        {
            "key": "{#34}│ {#36}󰇄 desktop {#keys}{#34}│",
            "type": "de"
        },
        {
            "key": "{#34}│ {#31} term    {#keys}{#34}│",
            "type": "terminal"
        },
        {
            "key": "{#34}│ {#32} shell   {#keys}{#34}│",
            "type": "shell"
        },
        {
            "key": "{#34}│ {#33}󰍛 cpu     {#keys}{#34}│",
            "type": "cpu",
            "showPeCoreCount": true
        },
        {
            "key": "{#34}│ {#34}󰾲 gpu     {#keys}{#34}│",
            "type": "gpu"
        },
        {
            "key": "{#34}│ {#35} disk    {#keys}{#34}│",
            "type": "disk",
            "folders": "/"
        },
        {
            "key": "{#34}│ {#36} memory  {#keys}{#34}│",
            "type": "memory"
        },
        {
            "key": "{#34}│ {#31}󰓡 swap    {#keys}{#34}│",
            "type": "swap"
        },
        {
            "key": "{#34}│ {#32}󰩟 network {#keys}{#34}│",
            "type": "localip",
            "format": "{ipv4} ({ifname})"
        },
        {
            "key": "{#34}│ {#33}{icon} battery {#keys}{#34}│",
            "type": "battery"
        },
        {
            "key": "{#34}├───────────┤",
            "type": "custom"
        },
        {
            "key": "{#34}│ {#39} colors  {#keys}{#34}│",
            "type": "colors",
            "symbol": "circle"
        },
        {
            "key": "{#34}╰───────────╯",
            "type": "custom"
        }
    ]
}
